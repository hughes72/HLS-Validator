<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="HLS1" packages="">
  <Appenders>
    <File name="MyLog" fileName="logs/HLS1.log">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="MyLog"/>
    </Root>
  </Loggers>
</Configuration>